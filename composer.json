{
	"name": "altis/core",
	"description": "Core module for Altis",
	"type": "composer-plugin",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Human Made",
			"email": "hello@humanmade.com"
		}
	],
	"repositories": {
		"rlankhorst/wp-consent-level-api": {
			"type": "vcs",
